  // }

  "React component connected": {
    "prefix": "rcc",
    "body": [
      "import * as React from 'react'",
      "import { connect } from 'react-redux'",
      "",
      "type Props = {}",
      "",
      "class $1 extends React.Component<Props> {",
      "  render() {",
      "    return <div>{this.props}</div>",
      "  }",
      "}",
      "",
      "export default connect(",
      "  (state: any) => ({}),",
      "  {},",
      ")($1)\n"
    ]
  },

  "React component": {
    "prefix": "rc",
    "body": [
      "import * as React from 'react'",
      "",
      "type Props = {}",
      "",
      "class $1 extends React.Component<Props> {",
      "  render() {",
      "    return <div>{this.props}</div>",
      "  }",
      "}\n",
      "export default $1\n"
    ]
  }
}
