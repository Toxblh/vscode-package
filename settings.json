{
  "clock.dateFormat": "dd.mm.yyyy HH:MM",
  "editor.fontFamily": "FuraCode Nerd Font",
  "editor.fontLigatures": true,
  "editor.fontSize": 14,
  "editor.tabSize": 4,
  "editor.minimap.enabled": false,
  "editor.formatOnSave": false,
  "extensions.autoUpdate": true,
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "files.useExperimentalFileWatcher": true,
  "workbench.iconTheme": "file-icons",
  "git.enableSmartCommit": true,
  "prettier.semi": false,
  "prettier.singleQuote": true,
  "prettier.eslintIntegration": true,
  "prettier.printWidth": 110,
  "gitlens.gitExplorer.files.layout": "tree",
  "diffEditor.ignoreTrimWhitespace": true,
  "gitlens.keymap": "alternate",
  "gitlens.advanced.messages": {
    "suppressCommitHasNoPreviousCommitWarning": false,
    "suppressCommitNotFoundWarning": false,
    "suppressFileNotUnderSourceControlWarning": false,
    "suppressGitVersionWarning": false,
    "suppressLineUncommittedWarning": false,
    "suppressNoRepositoryWarning": false,
    "suppressResultsExplorerNotice": false,
    "suppressShowKeyBindingsNotice": true
  },
  "gitlens.historyExplorer.enabled": true,
  "jest.pathToJest": "yarn test --",
  "jest.showCoverageOnLoad": true,
  "editor.formatOnType": true,
  "workbench.fontAliasing": "antialiased",
  "explorer.confirmDelete": false,
  "prettier.tslintIntegration": true,
  "window.zoomLevel": 0,
  "java.errors.incompleteClasspath.severity": "ignore",
  "typescriptHero.imports.insertSemicolons": false,
  "editor.renderWhitespace": "all",
  "gitlens.views.repositories.files.layout": "tree",
  "gitlens.views.fileHistory.enabled": true,
  "gitlens.views.lineHistory.enabled": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "javascript.preferences.quoteStyle": "single",
  "typescript.preferences.quoteStyle": "single",
  "tizentv.targetDeviceAddress": "192.168.1.3:26101",
  "tizentv.tizenStudioLocation": "~/tizen-studio",
  "tizentv.simulatorLocation": "~/tizen-studio",
  "tizentv.logLevel": "INFO",
  "tizentv.chromeExecutable": "/Applications/Google Chrome.app ",
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "breadcrumbs.enabled": true,
  "terminal.integrated.rendererType": "dom",
  "git.autofetch": true,
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "eslint.alwaysShowStatus": true,
  "eslint.packageManager": "yarn",
  "eslint.provideLintTask": true,
  "eslint.workingDirectories": ["./src"],
  "eslint.validate": ["javascript", "javascriptreact", "typescript", "typescriptreact"],
  "autoDarkMode.darkTheme": "One Dark Pro",
  "autoDarkMode.lightTheme": "Atom One Light",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "C_Cpp.updateChannel": "Insiders",
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "diffEditor.renderSideBySide": true,
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[cpp]": {
    "editor.defaultFormatter": "xaver.clang-format"
  },
  "git.confirmSync": false,
  "python.jediEnabled": false,
  "[yaml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "workbench.colorTheme": "Atom One Light",
}
